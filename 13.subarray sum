import java.io.*;
import java.util.*;
class subarr {
public static void main(String[] args) {
int[] arr = {-2, 1, -3, 4, -1, 2, 1, -5, 4};
int ans = arr[0];
int max1 = arr[0];

for (int i = 1; i < arr.length; i++) {
max1 = Math.max(arr[i], max1 + arr[i]);
ans = Math.max(ans, max1);
}

System.out.println("Maximum contiguous subarray sum is " + ans);
}
}
